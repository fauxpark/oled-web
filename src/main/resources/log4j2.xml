<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO">
	<appenders>
		<console name="Console" target="SYSTEM_OUT">
			<patternlayout pattern="%d{HH:mm:ss.SSS} %-5level %t [%c{1}] %msg%n"/>
		</console>
	</appenders>
	<loggers>
		<root level="info">
			<appenderref ref="Console"/>
		</root>
	</loggers>
</configuration>
